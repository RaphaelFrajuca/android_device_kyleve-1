# mount     	fstype  device                  [device2]   	[fstype2]   	[fsoptions]	[fsoptions2]

/boot		emmc     /dev/block/mmcblk0p5
/recovery	emmc	/dev/block/mmcblk0p6	NULL		NULL		NULL		NULL
/cache		ext4    /dev/block/mmcblk0p16	NULL        	rfs         	NULL		rw,nosuid,nodev,check=no
/data		ext4    /dev/block/mmcblk0p19	NULL        	rfs         	NULL		rw,nosuid,nodev,check=no
/sdcard		vfat    /devices/platform/sdhci.0/mmc_host/mmc2
/system		ext4    /dev/block/mmcblk0p17	NULL        	rfs         	NULL		rw,check=no
#/sd-ext	ext4    /dev/block/mmcblk0p2	NULL        	ext3        	NULL		rw,nosuid,nodev,check=no
